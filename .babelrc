{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets":{
                    "node":true
                },
				"useBuiltIns": "usage",
				"corejs": 3
            }
        ],
        [
			"@babel/preset-typescript",{
				"rewriteImportExtensions":true
			}
		]
    ],
	"plugins": [
		"tsconfig-alias-to-relative"
	]

}